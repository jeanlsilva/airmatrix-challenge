{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(t|j)sx?$": ["@swc/jest"]
  },
  "moduleNameMapper": { "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/assetsTransformer.js", "\\.(css|less)$": "<rootDir>/jest/assetsTransformer.js" },
